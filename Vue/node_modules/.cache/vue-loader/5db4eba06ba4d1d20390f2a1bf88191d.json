\n  createLivro,\n  updateLivro,\n  deleteLivro\n} from './livros'\n\nexport default {\n  components: { LivroForm },\n  setup () {\n    const mode = ref('view')\n    const current = ref(0)\n\n    const addLivro = () => {\n      createLivro()\n      mode.value = 'add'\n      current.value = size.value - 1\n    }\n\n    const updateChanges = (Livro) => {\n      updateLivro(Livro, current.value)\n      mode.value = 'view'\n    }\n\n    const removeLivro = (index) => {\n      deleteLivro(index)\n    }\n\n    const cancelChanges = () => {\n      if (mode.value === 'add') {\n        deleteLivro(size.value - 1)\n      }\n      mode.value = 'view'\n    }\n\n    return {\n      mode,\n      livros,\n      current,\n      addLivro,\n      updateChanges,\n      removeLivro,\n      cancelChanges\n    }\n  }\n}\n</script>\n"]}]}