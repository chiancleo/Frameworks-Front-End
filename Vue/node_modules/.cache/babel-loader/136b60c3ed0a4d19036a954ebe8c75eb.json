AJ,IADK;AAEL,MAAA,MAAM,EAAN,MAFK;AAGL,MAAA,OAAO,EAAP,OAHK;AAIL,MAAA,QAAQ,EAAR,QAJK;AAKL,MAAA,aAAa,EAAb,aALK;AAML,MAAA,WAAW,EAAX,WANK;AAOL,MAAA,aAAY,EAAZ;AAPK,KAAP;AASF;AArCa,CAAf","sourcesContent":["<template>\n  <div v-if=\"mode === 'view'\">\n    <p v-for=\"(livro, i) in livros\" :key=\"i\">\n        <b>Nome do livro: </b>{{livro.statement}}\n        <br />\n        <b>Autor: </b>{{livro.autor}}\n        <br />\n        <b>Link para Download: </b>\n        <!-- <a href=\"{{livro.imagem}}\" target=\"_blank\">{{livro.imagem}} </a> -->\n    <button @click=\"addLivro\">Adicionar Novo</button>\n    </p>\n  </div>\n  <div v-else>\n    <LivroForm\n      :livro=\"livros[current]\"\n      @update=\"updateChanges\"\n      @cancel=\"cancelChanges\"\n    />\n  </div>\n</template>\n\n<script>\nimport LivroForm from './LivroForm'\nimport { ref } from 'vue'\n\nimport {\n  livros,\n  size,\n  createLivro,\n  updateLivro,\n  deleteLivro\n} from './livros'\n\nexport default {\n  components: { LivroForm },\n  setup () {\n    const mode = ref('view')\n    const current = ref(0)\n\n    const addLivro = () => {\n      createLivro()\n      mode.value = 'add'\n      current.value = size.value - 1\n    }\n\n    const updateChanges = (Livro) => {\n      updateLivro(Livro, current.value)\n      mode.value = 'view'\n    }\n\n    const removeLivro = (index) => {\n      deleteLivro(index)\n    }\n\n    const cancelChanges = () => {\n      if (mode.value === 'add') {\n        deleteLivro(size.value - 1)\n      }\n      mode.value = 'view'\n    }\n\n    return {\n      mode,\n      livros,\n      current,\n      addLivro,\n      updateChanges,\n      removeLivro,\n      cancelChanges\n    }\n  }\n}\n</script>\n"],"sourceRoot":""}]}