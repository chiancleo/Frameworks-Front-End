add') {\n        deleteLivro(size.value - 1)\n      }\n      mode.value = 'view'\n    }\n\n    return {\n      mode,\n      livros,\n      current,\n      addLivro,\n      updateChanges,\n      removeLivro,\n      cancelChanges\n    }\n  }\n}\n</script>\n"],"sourceRoot":""}]}